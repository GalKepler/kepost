	pmcid = {PMC6095216},
	keywords = {Adolescent, Adult, Brain Mapping, Cerebral Cortex, Female, Humans, Magnetic Resonance Imaging, Male, Nerve Net, Young Adult},
	pages = {3095--3114},
	file = {Full Text:/home/galkepler/Zotero/storage/T57U6SVC/Schaefer et al. - 2018 - Local-Global Parcellation of the Human Cerebral Co.pdf:application/pdf},
